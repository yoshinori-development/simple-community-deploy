{
  "family": "simple-community-develop-api-main",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "taskRoleArn": "arn:aws:iam::858884198044:role/simple-community-develop-api-main-task-role",
  "executionRoleArn": "arn:aws:iam::858884198044:role/simple-community-develop-api-main-task-execution-role",
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "api",
      "image": "858884198044.dkr.ecr.ap-northeast-1.amazonaws.com/simple-community-api-main:develop-7434d26c7a5638b25d89319059b2f110de7e44b9",
      "portMappings": [
        {
          "protocol": "tcp",
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "main.cuashx4rqr4e.ap-northeast-1.rds.amazonaws.com"
        },
        {
          "name": "DB_USER",
          "value": "application"
        },
        {
          "name": "DB_DATABASE_NAME",
          "value": "main"
        }
      ],
      "secrets": [
        {
          "name": "DB_PASSWORD",
          "valueFrom": "/rds/main/develop/password"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": [],
        "options": {
          "awslogs-group": "/aws/ecs/simple-community-develop-api-main",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "api"
        }
      }
    }
  ]
}
