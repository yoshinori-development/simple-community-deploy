{
  "family": "simple-community-develop-api-core",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "taskRoleArn": "arn:aws:iam::278706633282:role/simple-community-develop-api-core-task-role",
  "executionRoleArn": "arn:aws:iam::278706633282:role/simple-community-develop-api-core-task-execution-role",
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "api",
      "image": "278706633282.dkr.ecr.ap-northeast-1.amazonaws.com/simple-community-api-core:1c76a6414e25f7c88514bdb8b4c98352482e7136",
      "portMappings": [
        {
          "protocol": "tcp",
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": ""
        },
        {
          "name": "DB_USER",
          "value": ""
        },
        {
          "name": "DB_DATABASE_NAME",
          "value": ""
        }
      ],
      "secrets": [
        {
          "name": "DB_PASSWORD",
          "valueFrom": "/simple-community/develop/rds/core/password"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": [],
        "options": {
          "awslogs-group": "/aws/ecs/simple-community-develop-api-core",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "api"
        }
      }
    }
  ]
}